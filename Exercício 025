#include <stdio.h>
#include <math.h>

int main() {
    float x1, y1;
    printf("Digite as coordenadas (x1, y1):\n");
    scanf("%f %f", &x1, &y1); 

    float x2, y2;
    printf("Digite as coordenadas (x2, y2):\n");
    scanf("%f %f", &x2, &y2); 

    float d;
    d = sqrt(pow(x1 - x2, 2) + pow(y1 - y2, 2));
    
    printf("Dist√¢ncia entre os pontos: %.2f\n", d);

    return 0;   
}
