#include <stdio.h>
#include <math.h>

int main() {
    float x, y, z;
    printf("Digite três números em ponto flutuante: \n");
    scanf("%f %f %f", &x, &y, &z); 

    float ma;
    ma = (x + y + z)/3;

    float mg;
    mg = pow(x * y * z, (1.0/3.0));
    
    printf("Média aritmética: %.2f\n", ma);
    printf("Média geométrica: %.2f\n", mg);
    
    return 0;   
}
