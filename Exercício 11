#include <stdio.h>

int main(){
    int x, y;
    printf("Digite dois números inteiros:\n");
    scanf("%d %d", &x, &y);

    printf("Soma: %d\n", x+y);
    printf("Produto: %d\n", x*y);
    printf("Diferença: %d\n", x-y);
    printf("Quociente: %d\n", x/y);
    printf("Resto da divisão: %d\n", x%y);

    return 0;
}
