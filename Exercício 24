#include <stdio.h>

int main() {
    int s;
    printf("Digite os segundos:\n");
    scanf("%d", &s); 

    int h;
    h = s/3600;
    s = s%3600;
    int m;
    m = s/60;
    s = s%60;

    printf("Horas: %d\n", h);
    printf("Minutos: %d\n", m);
    printf("Segundos: %d\n", s);

    return 0;   
}
